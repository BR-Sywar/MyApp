<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">



    <item android:width="325dp" android:height="60dp" android:name="inscrit">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#ff2a57ff" />
            <item android:name="android:shadowColor">#4d2a57ff</item>
            <!-- TextView shadow offsets do not respect scale. -->
            <item android:name="android:shadowDx">0</item>
            <item android:name="android:shadowDy">10</item>

        </shape>
    </item>

</selector>
